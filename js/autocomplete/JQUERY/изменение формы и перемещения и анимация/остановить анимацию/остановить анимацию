01	// Запускаем анимацию
02	$("#go").click(function(){
03	  $(".block").animate({left: '+=100px'}, 2000);
04	});
05	// Останавливаем анимацию
06	$("#stop").click(function(){
07	  $(".block").stop();
08	});
09	// Запускаем в другом направлении
10	$("#back").click(function(){
11	  $(".block").animate({left: '-=100px'}, 2000);
12	});
